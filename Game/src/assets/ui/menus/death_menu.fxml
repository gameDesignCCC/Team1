      xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <Label id="death-screen-header" alignment="CENTER" text="You Died">
        <VBox.margin>
          <Insets bottom="20.0"/>
        </VBox.margin>
    </Label>
    <Button id="button-wide" mnemonicParsing="false" onAction="#btnEventRestart" text="Restart">
        <VBox.margin>
          <Insets top="20.0"/>
        </VBox.margin>
    </Button>
    <Button id="button-wide" mnemonicParsing="false" onAction="#btnEventMainMenu" text="Main Menu">
        <VBox.margin>
          <Insets top="20.0"/>
        </VBox.margin>
    </Button>
</VBox>
